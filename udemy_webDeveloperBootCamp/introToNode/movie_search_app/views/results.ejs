<h1>Resutls Page!!!</h1>

<h3>results for searching : "<%= searchstring %>" </h3>
<ul>
    <% data.Search.forEach(function (movie){ %>
        <li>
            <img src=<%= movie.Poster %>width = "80" height ="80" > 
            <strong>
                <%= movie.Title %>
            </strong>
            (<%=movie.Year%>) 
        </li>
        
    
    <% }) %>
</ul>

<a href="/">search again</a>